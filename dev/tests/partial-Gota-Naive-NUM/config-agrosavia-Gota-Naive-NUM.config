# Configuration for agrosavia data filtered by plink
default:
    phenotypeFile    : "in/agrosavia-phenotype-checked.tbl"
    genotypeFile     : "in/agrosavia-genotype-checked.tbl"
    genotypeFormat   : "numeric"
    gwasModel        : "Naive"
    MIND             : 0.5      # Default 0.1
    GENO             : 0.5      # Default 0.1
    MAF              : 0.001     # Default 0.01
    HWE              : 1e-50    # Default 1e-10
